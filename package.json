    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.12"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint",
    "*": "npm run format"
  },
  "engines": {
    "node": ">=18.0.0"
  }
